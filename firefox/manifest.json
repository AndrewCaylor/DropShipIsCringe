{

  "description": "Links to Alibaba reverse image search. Copies Amazon product image URL to clipboard.",
  "manifest_version": 2,
  "name": "Easy Alibaba Image Search",
  "version": "1.0",
  "homepage_url": "https://github.com/AndrewCaylor/DropShipIsCringe",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.amazon.com/*"],
      "js": ["loader.js"]
    }
  ],
  "web_accessible_resources": [
    "button.html",
    "button.css",
    "button.js"
  ]
}